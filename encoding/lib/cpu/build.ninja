ninja_required_version = 1.3
cxx = c++

cflags = -DTORCH_EXTENSION_NAME=enclib_cpu -DTORCH_API_INCLUDE_EXTENSION_H -DPYBIND11_COMPILER_TYPE=\"_gcc\" -DPYBIND11_STDLIB=\"_libstdcpp\" -DPYBIND11_BUILD_ABI=\"_cxxabi1011\" -isystem /home/thanhnc/.local/lib/python3.8/site-packages/torch/include -isystem /home/thanhnc/.local/lib/python3.8/site-packages/torch/include/torch/csrc/api/include -isystem /home/thanhnc/.local/lib/python3.8/site-packages/torch/include/TH -isystem /home/thanhnc/.local/lib/python3.8/site-packages/torch/include/THC -isystem /home/thanhnc/miniconda3/envs/tuongnh/include/python3.8 -D_GLIBCXX_USE_CXX11_ABI=0 -fPIC -std=c++14
post_cflags = 
ldflags = -shared -L/home/thanhnc/.local/lib/python3.8/site-packages/torch/lib -lc10 -ltorch_cpu -ltorch -ltorch_python

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule link
  command = $cxx $in $ldflags -o $out

